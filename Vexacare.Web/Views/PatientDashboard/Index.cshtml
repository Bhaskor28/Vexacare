@using Vexacare.Domain.Enums
@model DashboardVM


@{
    Layout = "_Layout";
    ViewData["Title"] = "Patient Dashboard";
}

 

<!-- Dashboard Banner Section Start -->
    <section id="dashboard-banner">
        <div class="container">
            <div class="dashboard-banner-content">
                <h1 class="dashboard-banner-title">Dashboard</h1>
                <p class="dashboard-banner-description">
                    Track your gut health journey effortlessly and stay motivated by
                    following personalized, AI-powered recommendations designed to
                    improve your digestive wellness, boost overall vitality, and help
                    you achieve a balanced, healthier lifestyle every day.
                </p>
            </div>
        </div>
    </section>
    <!-- Dashboard Banner Section End -->
    <!-- Test Status Section Start -->
    <section id="test-status">
        <div class="container">
            <!-- IF no test ordered -->
        @if (Model.KitState == null || Model.KitState == KitState.None)   //if no test kit order ordered
        {
            <div class="test-status-wrapper">
                <div class="test-status-header">
                    <h3 class="test-status-title">Your Test Status</h3>
                </div>

                <div class="test-status-content">
                    <div class="test-status-icon">
                        <img src="~/assets/images/test-status-icon.png"
                        alt="Test Status Icon" />
                    </div>

                    <div class="test-status-text">
                        <p class="test-status-description">
                            You haven’t ordered your gut test kit yet. Start your journey
                            to better gut health today.
                        </p>

                        <a href="@Url.Action("Index", "Shop")" class="test-order-button">Order Your Kit</a>
                    </div>
                </div>
            </div>
        }
        else
        {
            <!-- IF test ordered -->
            <div class="text-status-wrapper text-status-wrapper-ordered">
                <div class="test-status-wrapper">
                    <div class="test-status-header">
                        <h3 class="test-status-title">Your Test Status</h3>

                        <a href="#" class="how-to-take">
                            <span><img src="~/assets/SVG/Playbutton.svg" alt="" /></span>
                            How to take the sample
                        </a>
                    </div>

                    <div class="test-status-steps">
                        @* step-1 *@
                        <div class="test-status-step">
                            <div class="test-status-icon compleate">
                                <img src="~/assets/SVG/TestStepCompleate1.svg"
                                     class="step-icon"
                                     alt="Test Status Icon" />
                            </div>

                            <p class="test-status-description">Kit Ordered</p>
                        </div>
                        @* Step-2 *@
                        <div class="test-status-step">
                            @if (Model.KitState == KitState.SampleReceived)
                            {
                                @if (Model.StateStatus == StateStatus.Done)
                                {
                                    <div class="test-status-icon compleate">
                                        <img src="~/assets/SVG/TestStepCompleate2.svg"
                                        class="step-icon"
                                        alt="Test Status Icon" />
                                    </div>
                                }
                                else if (Model.StateStatus == StateStatus.inProgress)
                                {
                                    <div class="test-status-icon pending">
                                        <img src="~/assets/SVG/TestStepPending2.svg"
                                        class="step-icon"
                                        alt="Test Status Icon" />
                                    </div>
                                }
                                else
                                {
                                    <div class="test-status-icon">
                                        <img src="~/assets/SVG/TestStepDefault2.svg"
                                        class="step-icon"
                                        alt="Test Status Icon" />
                                    </div>
                                }
                            }
                            else if ((int)Model.KitState > 2)
                            {
                                <div class="test-status-icon compleate">
                                    <img src="~/assets/SVG/TestStepCompleate2.svg"
                                         class="step-icon"
                                         alt="Test Status Icon" />
                                </div>
                            }
                            else
                            {
                                <div class="test-status-icon">
                                    <img src="~/assets/SVG/TestStepDefault2.svg"
                                         class="step-icon"
                                         alt="Test Status Icon" />
                                </div>
                            }
                            <p class="test-status-description">Sample Received</p>
                        </div>
                        @* step-3 *@
                        <div class="test-status-step">
                            @if (Model.KitState == KitState.InAnalysis)
                            {
                                @if (Model.StateStatus == StateStatus.Done)
                                {
                                    <div class="test-status-icon compleate">
                                        <img src="~/assets/SVG/TestStepCompleate3.svg"
                                        class="step-icon"
                                        alt="Test Status Icon" />
                                    </div>
                                }
                                else if (Model.StateStatus == StateStatus.inProgress)
                                {
                                    <div class="test-status-icon pending">
                                        <img src="assets/SVG/TestStepPending3.svg"
                                             class="step-icon"
                                             alt="Test Status Icon" />
                                    </div>
                                }
                                else
                                {
                                    
                                    <div class="test-status-icon">
                                        <img src="assets/SVG/TestStepDefault3.svg"
                                                class="step-icon"
                                                alt="Test Status Icon" />
                                    </div>
                                    
                                }
                            }
                            else if ((int)Model.KitState > 3)
                            {
                                <div class="test-status-icon compleate">
                                    <img src="~/assets/SVG/TestStepCompleate3.svg"
                                         class="step-icon"
                                         alt="Test Status Icon" />
                                </div>
                            }
                            else
                            {
                                <div class="test-status-icon">
                                    <img src="assets/SVG/TestStepDefault3.svg"
                                         class="step-icon"
                                         alt="Test Status Icon" />
                                </div>
                            }

                            <p class="test-status-description">In Analysis</p>
                        </div>

                        @* step-4 *@

                        <div class="test-status-step">
                            @if(Model.KitState == KitState.AIProcessing)
                            {
                                @if (Model.StateStatus == StateStatus.Done)
                                {
                                    <div class="test-status-icon compleate">
                                        <img src="~/assets/SVG/TestStepCompleate4.svg"
                                        class="step-icon"
                                        alt="Test Status Icon" />
                                    </div>
                                }
                                else if (Model.StateStatus == StateStatus.inProgress)
                                {
                                    <div class="test-status-icon pending">
                                        <img src="assets/SVG/TestStepPending4.svg"
                                             class="step-icon"
                                             alt="Test Status Icon" />
                                    </div>
                                }
                                else
                                {
                                    
                                    <div class="test-status-icon">
                                        <img src="assets/SVG/TestStepDefault4.svg"
                                                class="step-icon"
                                                alt="Test Status Icon" />
                                    </div>
                                    
                                }
                            }
                            else if ((int)Model.KitState > 4)
                            {
                                <div class="test-status-icon compleate">
                                    <img src="~/assets/SVG/TestStepCompleate4.svg"
                                         class="step-icon"
                                         alt="Test Status Icon" />
                                </div>
                            }
                            else
                            {

                                <div class="test-status-icon">
                                    <img src="assets/SVG/TestStepDefault4.svg"
                                         class="step-icon"
                                         alt="Test Status Icon" />
                                </div>

                            }
                            <p class="test-status-description">AI Processing</p>
                        </div>

                        @* step-5 *@

                        <div class="test-status-step">
                            @if(Model.KitState == KitState.MedicalValidation)
                            {
                                @if (Model.StateStatus == StateStatus.Done)
                                {
                                    <div class="test-status-icon compleate">
                                        <img src="~/assets/SVG/TestStepCompleate5.svg"
                                        class="step-icon"
                                        alt="Test Status Icon" />
                                    </div>
                                }
                                else if (Model.StateStatus == StateStatus.inProgress)
                                {
                                    <div class="test-status-icon pending">
                                        <img src="assets/SVG/TestStepPending5.svg"
                                             class="step-icon"
                                             alt="Test Status Icon" />
                                    </div>
                                }
                                else
                                {
                                    
                                    <div class="test-status-icon">
                                        <img src="assets/SVG/TestStepDefault5.svg"
                                                class="step-icon"
                                                alt="Test Status Icon" />
                                    </div>
                                    
                                }
                            }
                            else if ((int)Model.KitState > 5)
                            {
                                <div class="test-status-icon compleate">
                                    <img src="~/assets/SVG/TestStepCompleate5.svg"
                                         class="step-icon"
                                         alt="Test Status Icon" />
                                </div>
                            }
                            else
                            {

                                <div class="test-status-icon">
                                    <img src="assets/SVG/TestStepDefault5.svg"
                                         class="step-icon"
                                         alt="Test Status Icon" />
                                </div>

                            }

                            <p class="test-status-description">Medical Validation</p>
                        </div>

                        @* step-6 *@

                        <div class="test-status-step">
                            @if(Model.KitState == KitState.ResultsAvailable)
                            {
                                @if (Model.StateStatus == StateStatus.Done)
                                {
                                    <div class="test-status-icon compleate">
                                        <img src="~/assets/SVG/TestStepCompleate6.svg"
                                        class="step-icon"
                                        alt="Test Status Icon" />
                                    </div>
                                }
                                else if (Model.StateStatus == StateStatus.inProgress)
                                {
                                    <div class="test-status-icon pending">
                                        <img src="assets/SVG/TestStepPending6.svg"
                                             class="step-icon"
                                             alt="Test Status Icon" />
                                    </div>
                                }
                                else
                                {
                                    
                                    <div class="test-status-icon">
                                        <img src="assets/SVG/TestStepDefault6.svg"
                                                class="step-icon"
                                                alt="Test Status Icon" />
                                    </div>
                                    
                                }
                            }
                            else if ((int)Model.KitState > 6)
                            {
                                <div class="test-status-icon compleate">
                                    <img src="~/assets/SVG/TestStepCompleate6.svg"
                                         class="step-icon"
                                         alt="Test Status Icon" />
                                </div>
                            }
                            else
                            {

                                <div class="test-status-icon">
                                    <img src="assets/SVG/TestStepDefault6.svg"
                                         class="step-icon"
                                         alt="Test Status Icon" />
                                </div>

                            }
                            
                            <p class="test-status-description">Results Available</p>
                        </div>

                        <div style="width: 0;" class="test-status-line-bar"></div>
                    </div>
                </div>

                <div class="test-results-forecast box">
                    <h4 class="test-results-forecast-title">Results Forecast</h4>

                    <div class="test-results-forecast-date">
                        <span><img src="assets/SVG/Calander.svg" alt="" /></span>
                        <p>May 15, 2025</p>
                    </div>

                    <div class="test-results-forecast-button">
                        <a href="#" class="book-consultation-btn">Book a Consultation</a>
                    </div>
                </div>
            </div>
            }
        </div>
    </section>
    <!-- Test Status Section End -->
    <!-- Dashboard Tabs Section Start -->
    <section id="dashboard-tabs">
        <div class="container">
            <div class="dashboard-tabs-wrapper">
                <!-- Tab Navigations -->
                <ul class="nav nav-tabs" id="myTab" role="tablist">
                    <li class="nav-item" role="presentation">
                        <button class="nav-link active"
                                id="overview-tab"
                                data-bs-toggle="tab"
                                data-bs-target="#overview-tab-pane"
                                type="button"
                                role="tab"
                                aria-controls="overview-tab-pane"
                                aria-selected="true">
                            Overview
                        </button>
                    </li>
                    <li class="nav-item" role="presentation">
                        <button class="nav-link"
                                id="recommendations-tab"
                                data-bs-toggle="tab"
                                data-bs-target="#recommendations-tab-pane"
                                type="button"
                                role="tab"
                                aria-controls="recommendations-tab-pane"
                                aria-selected="false">
                            Recommendations
                        </button>
                    </li>
                    <li class="nav-item" role="presentation">
                        <button class="nav-link"
                                id="meal-planner-tab"
