                    <li class="nav-item" role="presentation">
                        <button class="nav-link"
                                id="notification-tab"
                                data-bs-toggle="tab"
                                data-bs-target="#notification-tab-pane"
                                type="button"
                                role="tab"
                                aria-controls="notification-tab-pane"
                                aria-selected="false">
                            Notification
                        </button>
                    </li>
                    <li class="nav-item" role="presentation">
                        <button class="nav-link"
                                id="profile-settings-tab"
                                data-bs-toggle="tab"
                                data-bs-target="#profile-settings-tab-pane"
                                type="button"
                                role="tab"
                                aria-controls="profile-settings-tab-pane"
                                aria-selected="false">
                            Profile Settings
                        </button>
                    </li>
                </ul>

                <!-- Tab Contents -->
                <div class="tab-content" id="myTabContent">
                    <!-- Overview Content Start -->
                    <div class="tab-pane fade show active"
                         id="overview-tab-pane"
                         role="tabpanel"
                         aria-labelledby="overview-tab"
                         tabindex="0">
                        <div id="overview-tab-content">
                            <!-- Last Update -->
                            <div class="last-update-data box">
                                <p class="last-update-data-description">
                                    All data shown here is derived from your most recent
                                    microbiota test, conducted on
                                    <span>February 12, 2025</span>.
                                </p>
                            </div>

                            <!-- Overall  Microbiota Score -->
                            <div class="overall-microbiota-score box">
                                <div class="overall-microbiota-score-top">
                                    <div class="overall-microbiota-score-top-left">
                                        <h5 class="overall-microbiota-score-title">
                                            Overall Microbiota Score
