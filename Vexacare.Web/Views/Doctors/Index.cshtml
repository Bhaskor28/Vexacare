<script>
    document.addEventListener("DOMContentLoaded", function () {
      // Set up toggle functionality for each day
      const days = [
        "monday",
        "tuesday",
        "wednesday",
        "thursday",
        "friday",
        "saturday",
        "sunday",
      ];

      days.forEach((day) => {
        const toggle = document.getElementById(`${day}-toggle`);
        const startInput = document.getElementById(`${day}-start`);
        const endInput = document.getElementById(`${day}-end`);
        const timeInputs = startInput.parentElement;
        const closedText = timeInputs.querySelector(".closed-text");

        // Initial setup based on toggle state
        updateInputsState();

        // Add event listener for toggle change
        toggle.addEventListener("change", updateInputsState);

        function updateInputsState() {
          if (toggle.checked) {
            startInput.disabled = false;
            endInput.disabled = false;
            closedText.style.display = "none";
            startInput.style.display = "inline-block";
            endInput.style.display = "inline-block";
            timeInputs.querySelector(".to-text").style.display =
              "inline-block";
          } else {
            startInput.disabled = true;
            endInput.disabled = true;
            closedText.style.display = "inline-block";
            startInput.style.display = "none";
            endInput.style.display = "none";
            timeInputs.querySelector(".to-text").style.display = "none";
          }
        }
      });

      // Set Tuesday to closed by default (as in the image)
      document.getElementById("tuesday-toggle").click();
    });
</script>

