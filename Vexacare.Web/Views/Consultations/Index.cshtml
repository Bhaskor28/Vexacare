            <section id="find-professional">
                <div class="container">
                    <div class="tm-section-header">
                        <h2 class="tm-section-heading">Find the Right Healthcare
                            Professional</h2>
                        <p class="tm-section-description">
                            Search by category, service type, location, and
                            availability
                        </p>
                    </div>

                    <form asp-controller="Consultations" asp-action="Index" class="tm-search-form" method="get">
                        <div class="tm-search-form-left">
                            <div class="tm-field">
                                <p class="tm-field-label">Category</p>
                                <select name="categoryId">
                                    <option value="">Choose a category</option>
                                @foreach (var category in ViewBag.Categories)
                                {
                                
                                <option value="@category.Id">@category.Name</option>
                                

                                }
                        @* <option value="nutritionist">Nutritionist</option>
                                    <option value="microbiota">Gut
                                        Microbiota</option>
                                    <option value="homeCare">Home Care</option> *@
                                </select>
                            </div>

                            <div class="tm-separator"></div>

                            <div class="tm-field">
                                <p class="tm-field-label">Service Type</p>
                                <select name="serviceTypeId">
                                    <option value="" >Choose a type</option>
                        @foreach(var servicetype in ViewBag.ServiceTypes)
                        {
                            <option value="@servicetype.Id">@servicetype.Name</option>
                        }
                        @* <option
                                        value="consultation">Consultation</option>
                                    <option value="homevisit">Home
                                        Visit</option> *@
                                </select>
                            </div>

                            <div class="tm-separator"></div>

                            <div class="tm-field">
                                <p class="tm-field-label">Location</p>
                                <select name="locationId">
                                    <option value="">Enter your location</option>
                                        @foreach(var location in ViewBag.Locations)
                                        {
                                            <option value="@location.Id">@location.Name</option>
                                        }
                                        <option value="ny">New York</option>
                                </select>
                            </div>

                            <div class="tm-separator"></div>

                            <div class="tm-field icon-field">
                                <p class="tm-field-label">Availability</p>
                                <div class="tm-field-icon-text">
                                    <i class="fas fa-calendar-alt"></i>
                                    <input name= "dateId" type="date" id="date-picker"
                                        placeholder="Date">
                                </div>
                            </div>
                        </div>

                        <button type="submit">
                            <i class="fas fa-search"></i> Search
                        </button>
                    </form>

                    <p class="tm-results-info">Showing @Model.Count() of @ViewBag.TotalItem</p>

                    <div class="tm-professionals-grid">

